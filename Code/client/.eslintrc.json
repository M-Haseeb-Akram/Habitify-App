{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
        "no-console": "off",
        "no-empty": 0,
        "no-irregular-whitespace": 0,
        "indent": ["error", 4]
    },
    "settings": {
        "angular": {
            "version": "detect"
        }
    }
}
